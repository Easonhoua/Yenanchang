				}
				.title{
					width:400rpx;
					height:86rpx;
					line-height:86rpx;
					background: rgba(134,96,51,.7);
					color:$white;
					font-size:$font36;
					bottom:-20rpx;
					left:0rpx;
				}
			}
			.right-imgbox{
				width:270rpx;
				height:300rpx;
				overflow:hidden;
				margin-left: 16rpx;
				.imgbox{
					width:270rpx;
					height:140rpx;
					.img{
						width:270rpx;
						height:140rpx;
					}
				}
				.bt20{
					margin-bottom:20rpx;
				}
			}
