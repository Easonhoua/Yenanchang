

// 发现南昌
.index{
	.headerNew{
		width: 100%;
		display: flex;
		padding:$space28;
		// height:140rpx;//状态栏高度不能写死
		height: calc(var(--status-bar-height) + 88rpx);
		align-items: center;
		justify-content: space-between;
		background-color: #FFFFFF;
		/* #ifdef H5 */
			padding-top:20rpx;
		/* #endif */
		/* #ifdef APP-PLUS */
			padding-top: calc(var(--status-bar-height) + 10rpx);;
		/* #endif */
		padding-bottom:10rpx;
		position: fixed;
		z-index: 100;
		box-sizing: border-box;
		.location{
			color:#000000;
			position: relative;
			padding-bottom: 10rpx;
			.icon{
				width:24rpx;
				height:30rpx;
				position: absolute;
				left:0;
				top:12rpx;
			}
			.city{
				font-size:$font36;
				color:#000000;
			}
			.date{
				font-size:$font24;
				color:#000000;
				.datetime{
					margin-right:26rpx;
					display: inline-block;
				}
			}
		}
		.weatherNew{
			font-size:$font46;
			.weather-icon{
				width:41rpx;
				height:34rpx;
				display: inline-block;
				margin-left:14rpx;
			}
		}
	}
	.slideboxNew{
	  width:100%;
	  position: relative;
	  .vr{
	   width:156rpx;
	   height:58rpx;
	   position: absolute;
	   top:170rpx;
	   right:30rpx;
	   z-index: 99;
	  }
	 }
	.user-statistics{
		padding:40rpx 28rpx 28rpx 28rpx;
		background: #fff;
		text-align: right;
		.icon{
			width:77rpx;
			height:72rpx;
			margin-right:10rpx;
		}
		.userbox{
			display: inline-block;
			position: relative;
			width:135rpx;
			margin-left:10rpx;
			.userlogo{
				width:62rpx;
				height:62rpx;
				border-radius: 100%;
				border:6rpx solid #fff;
				position: absolute;
				top:-40rpx;
				overflow: hidden;
				text-align: center;
			}
			.one{
				left:0;
			}
			.two{
				left:20rpx;
			}
			.three{
				left:40rpx;
			}
			.four{
				left:60rpx;
			}
			.five{
				left:80rpx;
			}
		}
		.userlogo .img{
			width:62rpx;
			height:62rpx;
			border-radius: 100%;
			display: inline-block;
		}
	}
	.module{
		.senic {
			@extend %flex;
			padding-bottom:5rpx;
			.item{
				width:312rpx;
				height:320rpx;
				margin-right:34rpx;
				.imgbox{
					width:312rpx;
					.img{
						height: 320rpx;
					}
				}
				.txt{
					width:100%;
					position: absolute;
					padding:20rpx;
					bottom:0;
					.item-title{
						color:$white;
					}
					.bottom-bar{
						color:$white;
					}
					.mark{
						border-radius: 6rpx;
						padding:2rpx 10rpx;
						color:#2A2835;
						background: rgba(255,255,255,.7);
						display: inline-block;
						font-size:$font24;
						margin-bottom:10rpx;
					}
					.location-icon{
						width:16rpx;
						height:20rpx;
						margin-right:8rpx;
					}
				}
			}
		}
		.event{
			flex-wrap:wrap;
			display: flex;
			.item{
				width:326rpx;
				height:430rpx;
				position: relative;
				margin:0 17rpx 34rpx 17rpx;
				@extend %box-show;
				.imgbox{
					width:326rpx;
					height:260rpx;
					.img{
						width:326rpx;
						height:260rpx;
					}
				}
				.txt{
					padding:20rpx;
					.item-title{
						height:80rpx;
					}
				}
			}
		}
	}
}
// 文化南昌
.culture{
	.search-form{
		padding:60rpx 28rpx 28rpx 28rpx;
		position: fixed;
		width:100%;
		background: #fff;
	}
	.cul-iconbox{
		@extend %flex-align-items;
		flex-wrap:wrap;
		justify-content: space-between;
		padding:0 28rpx;
		flex-wrap:wrap;
		.item{
			width:125rpx;
			text-align: center;
			margin-bottom:28rpx;
			.txt{
				color:$light-black;
			}
			.item-icon{
				width:100rpx;
				height:100rpx;
			}
		}
	}
	.subtitle{
		padding:28rpx;
		font-size:$font36;
		margin-top:17rpx;
	}
	.cul_tab{
		.tab{
			padding:28rpx 28rpx 0rpx 28rpx;
			font-size:28rpx;
			@extend %flex-align-items;
			.item{
				margin-right:50rpx;
				white-space: nowrap;
				&.active{
					font-size: $font36;
					color:$blue;
				}
			}
		}
	}
	.nearby{
		@extend %flex;
		justify-content: space-between;
		.left-imgbox{
			width:424rpx;
			height:340rpx;
			position: relative;
			.img{
				width:424rpx;
				height:340rpx;
				position: relative;
				border-radius: 5px;
			}
			.title{
				width:286rpx;
				height:60rpx;
				line-height:60rpx;
				text-align: center;
				background: rgba(255,255,255,.6);
				font-size:36rpx;
				position: absolute;
				bottom:0rpx;
				left:70rpx;
				display: inline-block;
				z-index: 1;
			}
		}
		.right-imgbox{
			width:252rpx;
			height:340rpx;
			.title{
				width:218rpx;
				height:40rpx;
				line-height:40rpx;
				text-align: center;
				background: rgba(255,255,255,.6);
				font-size:24rpx;
				position: absolute;
				bottom:-10rpx;
				left:18rpx;
				display: inline-block;
				z-index: 1;
			}
			.imgbox{
				width:252rpx;
				height:165rpx;
				position: relative;
				.img{
					width:252rpx;
					height:165rpx;
					border-radius: 5px;
				}
			}
		}
		
	}
}
.town{
	.search-form{
		padding:60rpx 28rpx 28rpx 28rpx;
		position: fixed;
		width:100%;
		background: #fff;
	}
	.module{
		margin-top:20rpx;
		.map{@extend %flex-justify-content;
				margin-bottom:50rpx;
			.left-imgbox{
				width:400rpx;
