}
// 发布动态
.publish{
	.header{
		width:100%;
		background: #fff;
		@extend %flex-justify-content;
		height:140rpx;
		padding:0 28rpx;
		padding-top:50rpx;
		font-size:$font36;
		position:fixed;
		z-index: 1;
		box-sizing: border-box;
		.close{
			width:40rpx;
			height:40rpx;
		}
	}
	.mapbox{
		width:100%;
		height:366rpx;
		background:#f1f1f;
		padding-top:140rpx;
	}
	.place{
		padding:28rpx 30rpx;
		@extend %box-show;
		margin-bottom:36rpx;
		.textarea{
			height:130rpx;
		}
	}
	.addimg{
		padding:170rpx 28rpx 28rpx 28rpx;
		.img{
			width:180rpx;
			height:180rpx;
		}
		.tip{
			color:$assist;
			margin-top:40rpx;
			display: inline-block;
		}
		.textarea-h {
			height: 80rpx;
			width: 680rpx;
			padding: 0rpx;
			background: #FFFFFF;
		}
		.textarea_showTip {
			text-align: right;
			margin-bottom: 10rpx;
			color: rgba(153, 153, 153, 1);
			padding-top: 20rpx;
		}
	}
	.addimg-m{
		padding-top:40rpx;
		padding-bottom:60rpx;
	}
	.module{
		.hot{
			.item{
				display: inline-block;
				white-space: nowrap;
				padding:8rpx 14rpx;
				@extend %border-radius;
				border:1rpx solid $blue;
				margin-right:24rpx;
				margin-bottom:24rpx;
				color:$blue;
			}
		}
		.destination{
			padding:24rpx 0;
			margin-bottom:22rpx;
			@extend %flex-justify-content;
			.icon{
				width:30rpx;
				height:30rpx;
				margin-right:8rpx;
				vertical-align: middle;
				margin-top:-8rpx;
			}
		}
		.destination-bx{
			@extend %box-show;
			padding:28rpx;
		}
		.spot{
			margin-bottom:30rpx;
			.blue{
				color:$blue;
				margin-bottom:28rpx;
			}
			.scenic{
				display: block;
			}
			.spot-destination{
				margin-top:20rpx;
			}
		}
	}
	.des-listbox{
		margin-top:220rpx;
		.tip{
			color:$blue;
			padding:10rpx 52rpx;
		}
		.des-list{
			padding:28rpx 60rpx;
			.destination{
				font-size:$font28;
			}
			.address{
				font-size:$font24;
				color:#B8B4A9;
			}
		}
	}
	.tabbar{
		padding:0 70rpx;
		height:98rpx;
		@extend %flex-justify-content;
		.item{
			text-align: center;
			.icon{
				width:44rpx;
				height:44rpx;
				display: inline-block;
			}
			.dongtai{
				background: url(~@/static/img_new/main/f_fabu_nor@3x.png) no-repeat;
				background-size:44rpx 44rpx ;
			}
			.mudidi{
				background: url(~@/static/img_new/main/f_mudidi_nor@3x.png) no-repeat;
				background-size:44rpx 44rpx ;
			}
			.xianlu{
				background: url(~@/static/img_new/main/f_luxian_nor@3x.png) no-repeat;
				background-size:44rpx 44rpx ;
			}
			&.active{
				.txt{
					color:$blue;
				}
				.dongtai{
					background: url(~@/static/img_new/main/f_fabu_pre@3x.png) no-repeat;
					background-size:44rpx 44rpx ;
				}
				.mudidi{
					background: url(~@/static/img_new/main/f_mididi_pre@3x.png) no-repeat;
					background-size:44rpx 44rpx ;
				}
				.xianlu{
					background: url(~@/static/img_new/main/f_luxian_pre@3x.png) no-repeat;
					background-size:44rpx 44rpx ;
				}
			}
		}
	}
}
.choosedestination{
	.header{
		z-index: 99;
		top:0;
		height:220rpx;
		display: inherit;
		.title{
			font-size:$font36;
			width:100%;
			text-align: center;
			position: relative;
			.close{
				position: absolute;
				left:0rpx;
			}
			.rightbtn{
				position: absolute;
				right:0rpx;
			}
		}
	}
}
// 个人中心
.user{
	padding:0 28rpx;
	.herder{
		@extend %flex-justify-content;
		padding-top:50rpx;
		.set{
			width:40rpx;
			height:40rpx;
		}
		.notice{
			width:40rpx;
			height:40rpx;
		}
	}
	.userpanel{
		height:auto;
		box-shadow: 0rpx 4rpx 16rpx rgba(13,25,43,.1);
		padding:30rpx;
		@extend %border-radius;
		// margin-top:50rpx;
		margin-top: calc(var(--status-bar-height) + 88rpx + 20rpx);
		margin-bottom:60rpx;
		.user{
			padding:40rpx 20rpx 40rpx 20rpx;
			.userlogo{
				width:120rpx;
				height:120rpx;
				border-radius: 100%;
				vertical-align: middle;
				margin-top:-15rpx;
				margin-right:36rpx;
			}
			.name{
				font-size:$font36;
				color:$blue;
				font-weight:650;
			}
			.edit{
				width:40rpx;
				height:40rpx;
				margin-left:20rpx;
				vertical-align: middle;
				margin-top:-6rpx;
			}
		}
		.hobby{
			margin-top:0rpx;
			margin-bottom:38rpx;
			.title{
				margin-bottom:10rpx;
			}
			.mark{
				display: inline-block;
				background: #866033;
				color:$white;
				@extend %border-radius;
				margin-right:20rpx;
				font-size:$font24;
				padding:4rpx 16rpx;
				white-space: nowrap;
			}
		}
	}
	.column{
		@extend %flex-justify-content;
		padding:40rpx 0 20rpx 0;
		.item{
			text-align: center;
			.icon{
				width:50rpx;
				height:50rpx;
			}
			.txt{
				font-size:$font24;
			}
		}
	}
	.tab-user{
		.tab{
			@extend %flex-justify-content;
			padding:0rpx 72rpx;
			margin-bottom:34rpx;
			.item{
				text-align: center;
				.icon{
					width:50rpx;
					height:50rpx;
					display: block;
					margin:0 auto;
					margin-bottom:10rpx;
				}
				.tuwen{
					background: url(~@/static/img_new/main/my_tuwen_pre@3x.png) no-repeat;
					background-size: 50rpx 50rpx;
				}
				.dongtai{
					background: url(~@/static/img_new/main/my_dongtai_nor@3x.png) no-repeat;
					background-size: 50rpx 50rpx;
				}
				.mudidi{
					background: url(~@/static/img_new/main/my_mudidi_nor@3x.png) no-repeat;
					background-size: 50rpx 50rpx;
				}
				.xianlu{
					background: url(~@/static/img_new/main/my_xianlu_nor@3x.png) no-repeat;
					background-size: 50rpx 50rpx;
				}
				.txt{
					display: inline-block;
					font-size:$font24;
				}
				&.active{
					.txt{
						color:$blue;
					}
					.tuwen{
						background: url(~@/static/img_new/main/my_tuwen_pre@3x.png) no-repeat;
						background-size: 50rpx 50rpx;
					}
				}
			}
		}
		.module{
			padding:0;
		}
	}
}
// 文化南昌地点列表
.placelist{
	.search-form{
		padding:0 28rpx 28rpx 28rpx;
		position: fixed;
		width:100%;
		background: #fff;
	}
	.tab{
		width:100%;
		display: flex;
		position: fixed;
		top:140rpx;
		padding-top:40rpx;
		padding-bottom:30rpx;
		/* #ifdef APP-PLUS */
		top: 60rpx;
		padding-top:30rpx;
		/* #endif */
		justify-content:center;
		background: #fff;
		z-index: 3;
		.item{
			margin:10rpx 24rpx;
			color:$light-black;
			&.active{
				color:$black;
				border-bottom:6rpx solid $black;
			}
		}
		.classify{
			color:$black;
			position: relative;
			.i{
				width:16rpx;
				height:12rpx;
				display: inline-block;
				margin-left:8rpx;
				&.up{
					background: url(~@/static/img_new/common/arrow_up@3x.png) no-repeat;
					background-size:16rpx 12rpx ;
				}
				&.down{
					background: url(~@/static/img_new/common/arrow_down@3x.png) no-repeat;
					background-size:16rpx 12rpx ;
				}
			}
			.sub-classify{
				width:180rpx;
				position: absolute;
				padding:28rpx;
				background: #fff;
				@extend %box-show;
				left:50%;
				top:52rpx;
				margin-left:-100rpx;
				.subitem{
					padding:10rpx 0;
					color: $light-black;
					.icon{
						width:40rpx;
						height:40rpx;
						margin-right:5rpx;
						vertical-align: middle;
						margin-top:-10rpx;
					}
				}
			}
		}
	}
	.tab-con{
		.mapbox{
			background: #f3f5f7;
			height:340rpx;
		}
		.module{
			.play{
				display: inherit;
				.item{
					width:100%;
					margin-right:0;
					border-radius: 20rpx;
					overflow: hidden;
					height:500rpx;
					margin-bottom:34rpx;
					.imgbox{
						width:100%;
						height:360rpx;
						.img{
							width:100%;
							height:360rpx;
						}
					}
					.txt{
						.item-title{
							font-weight:normal;
							padding-top:10rpx;
						}
					}
					.bottom-bar{
						.dingwei{
							width:30rpx;
							height:30rpx;
							margin-right:6rpx;
							vertical-align: middle;
							margin-top:-8rpx;
							display: inline-block;
						}
					}
				}
			}
		}
	}
}

