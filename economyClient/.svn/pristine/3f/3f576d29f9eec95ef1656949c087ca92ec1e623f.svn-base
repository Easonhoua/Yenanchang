/**单选按钮**/
radio {
	position: relative;
}

radio::before {
	font-family: "cuIcon";
	content: "\e645";
	position: absolute;
	color: #ffffff !important;
	top: 50%;
	margin-top: -8px;
	right: 5px;
	font-size: 32upx;
	line-height: 16px;
	pointer-events: none;
	transform: scale(1, 1);
	transition: all 0.3s ease-in-out 0s;
	z-index: 9;
}

radio .wx-radio-input,
radio .uni-radio-input {
	/* margin: 0; */
	/* #ifdef APP-PLUS */
	margin-right: 0;
	width: 15px;
	height: 15px;
	/* #endif */
	/* #ifdef H5 */
	margin-right: 10rpx;
	width: 14px;
	height: 14px;
	/* #endif */
}

radio-group {
	display: inline-block;
}

radio.radio::before,
radio .wx-radio-input::before,
radio.radio::before,
radio .uni-radio-input::before {
	display: none;
}

radio.radio[checked]::after,
radio.radio .uni-radio-input-checked::after {
	content: "";
	background-color: transparent;
	display: block;
	position: absolute;
	/* #ifdef APP-PLUS */
	width: 9px;
	height: 9px;
	top: 7upx;
	/* #endif */
	/* #ifdef H5 */
	width: 10px;
	height: 10px;
	top: 0upx;
	/* #endif */
	z-index: 999;
	left: 0upx;
	right: 0;
	bottom: 0;
	margin: auto;
	border-radius: 200upx;
	/* #ifndef MP */
	/* #ifdef APP-PLUS */
	border: 2px solid #ffffff !important;
	/* #endif */
	/* #ifdef H5 */
	border: 1px solid #ffffff !important;
	/* #endif */
	/* #endif */

	/* #ifdef MP */
	border: 1px solid #ffffff !important;
	/* #endif */
}

