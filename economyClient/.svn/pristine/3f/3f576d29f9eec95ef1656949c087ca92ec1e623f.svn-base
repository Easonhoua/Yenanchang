.clear {
	clear: both;
	margin: 0;
	padding: 0
}

.clearfix:after {
	clear: both;
	display: block;
	visibility: hidden;
	height: 0;
	content: " ";
	font-size: 0
}

* html .clearfix {
	zoom: 1
}

.Text-hid {
	display: block;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;

}

.Text-hid2 {
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	/*超出两行显示省略号*/
}

.Text-hid3 {
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 3;
	/*超出三行显示省略号*/

}

